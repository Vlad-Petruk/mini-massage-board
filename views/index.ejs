<html>
  <head>
    <link rel="stylesheet" href="../styles.css">
    <title>Homepage</title>
  </head>
  <body>
    <h1><%= title %></h1>
    <% for (let i = 0; i < messages.length; i++) { %>
      <div>
        <p><%= messages[i].username %></p>
        <p><%= messages[i].text %></p>
        <a href="/message/<%= messages[i].id %>">
          <button type="button">Open message</button>
        </a>
      </div>
    <% } %>

    <a href="<%= links[1].href %>">
        <span><%= links[1].text %></span>
    </a>
  </body>
</html>